x=float(input("Enter x-axis:"))
y=float(input("Enter y-axis:"))
if((x>0 and y>0) or (x>=0 and y>0) or (x>0 and y>=0)):
    print("Quadrant 1")
elif((x<0 and y>0) or (x<=0 and y>0) or (x<0 and y>=0)):
    print("Quadrant 2")
elif((x<0 and y<0) or (x<=0 and y<0) or (x<0 and y<=0)):
    print("Quadrant 3")
elif((x>0 and y<0) or (x>=0 and y<0) or (x>0 and y<=0)):
    print("Quadrant 4")
else:
    print("Origin")
