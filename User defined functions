def add(a,b):           
    c=a+b
    print(c)
def mul(a,b):
    c=a*b
    print(c)
def sub(a,b):
    c=a-b
    print(c)
def div(a,b):
    c=a/b
    print(c)
def fdiv(a,b):
    c=a//b
    print(c)
def mod(a,b):
    c=a%b
    print(a,b)
m=int(input())
n=int(input())
add(m,n)
sub(m,n)
mul(m,n)
div(m,n)
fdiv(m,n)
mod(m,n)
